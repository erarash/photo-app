{
  "name": "PhotoApp",
  "version": "1.0",
  "description": "save your online Photos",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["bundle.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}
